<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/txtRoe"
        android:layout_width="match_parent"
        android:layout_height="79dp"
        android:fontFamily="sans-serif-condensed"
        android:paddingLeft="20dp"
        android:text="TextView"
        android:textColor="#454545"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/txtVpa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtPvp"
        android:layout_width="match_parent"
        android:layout_height="79dp"
        android:layout_marginBottom="92dp"
        android:fontFamily="sans-serif-condensed"
        android:paddingLeft="20dp"
        android:text="TextView"
        android:textColor="#454545"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtLpa"
        android:layout_width="match_parent"
        android:layout_height="79dp"
        android:fontFamily="sans-serif-condensed"
        android:paddingLeft="20dp"
        android:text="TextView"
        android:textColor="#454545"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/txtPl"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtVpa"
        android:layout_width="match_parent"
        android:layout_height="79dp"
        android:fontFamily="sans-serif-condensed"
        android:paddingLeft="20dp"
        android:text="TextView"
        android:textColor="#454545"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/txtPvp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtAtivoResultado"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:fontFamily="sans-serif-medium"
        android:text="TextView"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#292B32"
        android:textSize="50sp"
        app:layout_constraintBottom_toTopOf="@+id/txtLpa"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtPl"
        android:layout_width="match_parent"
        android:layout_height="76dp"
        android:fontFamily="sans-serif-condensed"
        android:paddingLeft="20dp"
        android:text="TextView"
        android:textColor="#454545"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/txtRoe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>